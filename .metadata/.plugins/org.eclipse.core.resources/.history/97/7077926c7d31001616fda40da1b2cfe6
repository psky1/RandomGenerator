		public class MathTwo {
			 public static void main(String[] args) {
			 int mynumber;
			 float myothernumber;
			 
			 mynumber = 2;
			 myothernumber = 1.7938f;
			 
			
			 System.out.println("my number = " + mynumber);
			 System.out.println("my other number " + myothernumber);
			 }
			

	}

		