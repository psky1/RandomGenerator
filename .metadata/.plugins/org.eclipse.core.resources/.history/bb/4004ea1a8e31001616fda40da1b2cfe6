
public class MoreEscapeSequences {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		System.out.print( "Cost\t\t Quantity\t\t Total\t\t\n");
		System.out.print( "$1,000\t\t   4\t\t\t $4,000      *\n");
		System.out.print( "*      *\n");
		System.out.print( "*      *\n");
		System.out.print( "*      *\n");
		System.out.print( "********\n");
		
		
	}

}


//Cost      Quantity   Total
//$1,000.00   4       $4,000.00
//$   50.00   8         $400.00
//______________________________
//TOTAL:      12      $4,400.00